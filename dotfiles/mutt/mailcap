application/pdf;  zathura %s &;
application/vnd.openxmlformats-officedocument.wordprocessingml.document; loffice %s &;
application/vnd.openxmlformats-officedocument.spreadsheetml.sheet; loffice %s &;
image/gif;        mpv --loop %s &;
image/*;          sxiv %s &;
text/calendar;    .local/bin/vcal -all -; copiousoutput;
text/html;        mv %s /tmp/mail.html && $BROWSER /tmp/mail.html;
text/html;        elinks -dump; copiousoutput;
text/*;           $PAGER %s ;
video/*;          mpv %s &;
